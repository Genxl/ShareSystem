<div class="pageContent">
	<table class="table" width="100%"  border="1" layoutH="48" align="center" cellspacing="0" bordercolor="#999999" style="border-collapse:collapse">
		<thead>
			<tr>
				<th width="50">ID</th>
				<th width="150">用户名</th>
				<th width="80">分类</th>
				<th>文档标题</th>
				<th width="100">下载时间</th>
				<th width="30">删除</th>
			</tr>
		</thead>
		<tbody>
      <#list model.downLogs as d>
      <tr align="center">
      	 <td>${d.id}</td>
		 <td>${d.user.name?default('')}</td>
		 <td>${d.document.category.title?default('')}</td>
		 <td><a href='${base}/document/view?id=${d.document.id}' target="_blank">${d.document.title}</a></td>
		 <td>${d.downTime?string('yyyy-MM-dd')}</td>
		 <td>
      	 	<a href="${base}/ad_min/document/download/del?doc_id=${d.id}" target="ajaxTodo" title="删除这个记录将不能恢复，执行吗?">删除</a>
		 </td>
      </tr>
	</#list>
	</tbody>
    </table>
	<div class="panelBar">
		<div class="pages">
			<span>共${model.totalCount}条</span>
		</div>
		<div class="pagination" targetType="navTab" totalCount="${model.totalCount}" numPerPage="${model.pageSize}" pageNumShown="16" currentPage="${model.page}"></div>
	</div>
</div>
