<script>
	//使用服务器端网络进行搜索，修改分页信息，传递type参数
	//集成显卡 2012
	var t=${type};
	var list=document.getElementsByTagName('a');
	for(var i=0;i<list.length;i++){
		list[i].href=list[i].href+"&type="+t;
	}
</script>
