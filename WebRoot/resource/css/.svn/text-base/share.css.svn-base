/*
 * =====================================================
 * 动画效果
 * =====================================================
 */
ul.item {
	margin-bottom: 3em;
}
.roundabout-holder {
	width: 400px;
	height: 5em;
	overflow: hidden;
	margin: 0;
	padding: 0;
	position: relative;
	list-style: none;
	top: -5px;
	float:left;
}

.roundabout-moveable-item {
	width: 400px;
	height: 1.5em;
	line-height: 1.5;
	font-size: 16px;
	text-align: center;
	cursor: pointer;
	color: #329;
	font-family: Chalkboard, Comic Sans, awful-awful-font;
}

.roundabout-in-focus {
	cursor: auto;
	text-decoration:underline;
	font-weight:700
}

.monolith { 
	background-color: #000;
	position: absolute;
	width: 17em;
	top: 0em;
	left: 7.5em;
	z-index: 200;
	height: 40em;
}

.monolith .pad {
	padding: 1em; 
	text-align: center;
	color: #999;
}
			