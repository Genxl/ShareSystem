package cn.yws.longan.domain.share.article;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

import cn.yws.longan.domain.BaseEntity;
import cn.yws.longan.domain.User;

/**
 * @项目名称 :longan
 * @文件名称 :ArtComment.java
 * @所在包 :cn.yws.longan.domain.share.article
 * @功能描述 :
 *	文章评论
 * @创建者 :集成显卡	1053214511@qq.com
 * @创建日期 :2012-4-11
 * @修改记录 :
 */
@Entity
@Table(name="share_article_comment")
@SuppressWarnings("serial")
public class ArtComment extends BaseEntity{
	private User user;
	private Article article;
	private String content;
	private Date date;
	
	@ManyToOne(fetch=FetchType.LAZY)
	@JoinColumn(name="user_id",nullable=false)
	public User getUser() {
		return user;
	}
	public void setUser(User user) {
		this.user = user;
	}
	
	@ManyToOne(fetch=FetchType.LAZY)
	@JoinColumn(name="article_id",nullable=false)
	public Article getArticle() {
		return article;
	}
	public void setArticle(Article article) {
		this.article = article;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
}
